server {
        listen 81 default_server;
        listen [::]:81 default_server;
        listen 444 ssl;
        root /var/www/html;
        index index.html index.htm index.nginx-debian.html;
        server_name {{ server_name }};
        ssl_certificate {{ cert_file }};
        ssl_certificate_key {{ key_file }};
        location / {
                try_files $uri $uri/ =404;
        }
}
